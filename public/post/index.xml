    <item>
      <title>Status Aktivasi</title>
      <link>http://localhost:1313/post/checkactivationstatus/</link>
      <pubDate>Wed, 25 Jun 2025 00:00:00 +0000</pubDate>
      <guid>http://localhost:1313/post/checkactivationstatus/</guid>
      <description>&lt;h3 id=&#34;check-activation-status&#34;&gt;Check Activation Status&lt;/h3&gt;&#xA;&lt;p&gt;Skrip Periksa Status Aktivasi di MAS merupakan pengembangan dari CAS oleh abbodi1406.&#xA;Anda dapat membaca lebih lanjut tentang fitur-fiturnya &lt;a href=&#34;../cas&#34;&gt;disni&lt;/a&gt;.&lt;/p&gt;&#xA;&lt;div class=&#34;rounded overflow-hidden border border-slate-300 bg-slate-100 dark:border-slate-700 dark:bg-slate-800 relative group text-xs&#34;&gt;&#xD;&#xA;  &#xD;&#xA;    &lt;div class=&#34;px-3 py-1.5 font-semibold bg-slate-200 dark:bg-slate-700 text-slate-900 dark:text-white border-b border-slate-300 dark:border-slate-600&#34;&gt;&#xD;&#xA;      Contoh Golang&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;  &#xD;&#xA;&#xD;&#xA;  &lt;button onclick=&#34;copyCode(this)&#34; &#xD;&#xA;          class=&#34;absolute top-1.5 right-1.5 text-[10px] px-1.5 py-0.5 bg-emerald-500 hover:bg-emerald-600 text-white rounded shadow hidden group-hover:block transition&#34;&gt;&#xD;&#xA;    Copy&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;pre class=&#34;bg-white dark:bg-slate-900 px-3 py-2 text-[13px] text-slate-800 dark:text-slate-100 overflow-auto&#34;&gt;&#xD;&#xA;    &lt;code class=&#34;language-go&#34;&gt;&#xD;&#xA;package main&#xD;&#xA;&#xD;&#xA;import &#34;fmt&#34;&#xD;&#xA;&#xD;&#xA;func main() {&#xD;&#xA;    fmt.Println(&#34;Halo Dunia&#34;)&#xD;&#xA;}&#xD;&#xA;&lt;/code&gt;&#xD;&#xA;  &lt;/pre&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xA;&lt;div class=&#34;simple-tabs&#34;&gt;&#xD;&#xA;  &lt;div class=&#34;tab-buttons&#34;&gt;&#xD;&#xA;    &#xD;&#xA;      &lt;button &#xD;&#xA;        class=&#34;tab-button active&#34; &#xD;&#xA;        onclick=&#34;switchTab(this, &#39;tab-0&#39;)&#34;&#xD;&#xA;      &gt;&#xD;&#xA;        Test A&#xD;&#xA;      &lt;/button&gt;&#xD;&#xA;    &#xD;&#xA;      &lt;button &#xD;&#xA;        class=&#34;tab-button &#34; &#xD;&#xA;        onclick=&#34;switchTab(this, &#39;tab-1&#39;)&#34;&#xD;&#xA;      &gt;&#xD;&#xA;        Test B&#xD;&#xA;      &lt;/button&gt;&#xD;&#xA;    &#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;  &lt;div class=&#34;tab-contents&#34;&gt;&#xD;&#xA;    &#xD;&#xA;  &lt;div id=&#34;tab-0&#34; class=&#34;tab-content active&#34;&gt;&#xD;&#xA;  Isi A&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;  &lt;div id=&#34;tab-1&#34; class=&#34;tab-content &#34;&gt;&#xD;&#xA;  Isi B&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xA;&lt;details class=&#34;alert-details&#34;&gt;&#xD;&#xA;  &lt;summary&gt;Metode 1&lt;/summary&gt;&#xD;&#xA;  &lt;div class=&#34;details-content&#34;&gt;&#xD;&#xA;    contoh details&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&lt;/details&gt;&#xA;&lt;div class=&#34;custom-alert custom-alert-warning&#34; role=&#34;alert&#34;&gt;&#xD;&#xA;  &lt;div class=&#34;custom-alert-header&#34;&gt;&lt;i class=&#34;custom-alert-icon fas fa-triangle-exclamation&#34;&gt;&lt;/i&gt;&lt;span class=&#34;custom-alert-title&#34;&gt;Perhatian&lt;/span&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;  &lt;div class=&#34;custom-alert-content&#34;&gt;&#xD;&#xA;    &lt;ul&gt;&#xA;&lt;li&gt;type=&amp;ldquo;tip&amp;rdquo; untuk saran praktis&lt;/li&gt;&#xA;&lt;li&gt;type=&amp;ldquo;note&amp;rdquo; untuk catatan teknis&lt;/li&gt;&#xA;&lt;li&gt;type=&amp;ldquo;warning&amp;rdquo; untuk peringatan risiko&lt;/li&gt;&#xA;&lt;li&gt;type=&amp;ldquo;danger&amp;rdquo; untuk konsekuensi kritis&lt;/li&gt;&#xA;&lt;li&gt;type=&amp;ldquo;info&amp;rdquo; untuk keterangan umum&lt;/li&gt;&#xA;&lt;/ul&gt;&#xA;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&lt;/div&gt;&#xA;&lt;div class=&#34;content-source&#34;&gt;&#xD;&#xA;  &lt;a href=&#34;https://example.com/sumber-artikel&#34; target=&#34;_blank&#34; rel=&#34;noopener noreferrer&#34; class=&#34;source-link&#34;&gt;&#xD;&#xA;    Sumber: Nama Website/Sumber&#xD;&#xA;  &lt;/a&gt;&#xD;&#xA;&lt;/div&gt;&#xA;&#xD;&#xA;  &lt;figure class=&#34;my-6 text-center&#34;&gt;&#xD;&#xA;    &lt;img &#xD;&#xA;      src=&#34;http://localhost:1313/path%20image&#34; &#xD;&#xA;      alt=&#34;Deskripsi gambar&#34; &#xD;&#xA;      class=&#34;mx-auto cursor-zoom-in max-w-full rounded-lg shadow-md transition-all hover:shadow-lg&#34;&#xD;&#xA;      onclick=&#34;zoomImage(this)&#34;&#xD;&#xA;      loading=&#34;lazy&#34;&#xD;&#xA;      decoding=&#34;async&#34;&#xD;&#xA;    &gt;&#xD;&#xA;    &#xD;&#xA;    &lt;figcaption class=&#34;text-sm text-gray-500 dark:text-gray-400 mt-3 px-4&#34;&gt;&#xD;&#xA;      &lt;em&gt;Gambar contoh dengan zoom&lt;/em&gt;&#xD;&#xA;    &lt;/figcaption&gt;&#xD;&#xA;    &#xD;&#xA;  &lt;/figure&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;script&gt;&#xD;&#xA;  function zoomImage(img) {&#xD;&#xA;    const isZoomed = img.classList.toggle(&#39;zoomed&#39;);&#xD;&#xA;    &#xD;&#xA;    if (isZoomed) {&#xD;&#xA;      &#xD;&#xA;      const viewportPadding = 80; &#xD;&#xA;      const scale = Math.min(&#xD;&#xA;        (window.innerWidth - viewportPadding) / img.naturalWidth,&#xD;&#xA;        (window.innerHeight - viewportPadding) / img.naturalHeight,&#xD;&#xA;        1.0 &#xD;&#xA;      );&#xD;&#xA;      &#xD;&#xA;      img.style.setProperty(&#39;--zoom-scale&#39;, scale);&#xD;&#xA;      document.documentElement.classList.add(&#39;zoom-active&#39;);&#xD;&#xA;      &#xD;&#xA;      &#xD;&#xA;      const handleKey = (e) =&gt; {&#xD;&#xA;        if (e.key === &#39;Escape&#39; || e.keyCode === 27) {&#xD;&#xA;          zoomImage(img);&#xD;&#xA;        }&#xD;&#xA;      };&#xD;&#xA;      &#xD;&#xA;      &#xD;&#xA;      const handleClick = (e) =&gt; {&#xD;&#xA;        if (e.target === img) return;&#xD;&#xA;        zoomImage(img);&#xD;&#xA;      };&#xD;&#xA;      &#xD;&#xA;      document.addEventListener(&#39;keydown&#39;, handleKey);&#xD;&#xA;      document.addEventListener(&#39;click&#39;, handleClick);&#xD;&#xA;      &#xD;&#xA;      &#xD;&#xA;      img.dataset.escHandler = handleKey;&#xD;&#xA;      img.dataset.clickHandler = handleClick;&#xD;&#xA;      &#xD;&#xA;    } else {&#xD;&#xA;      document.documentElement.classList.remove(&#39;zoom-active&#39;);&#xD;&#xA;      &#xD;&#xA;      &#xD;&#xA;      if (img.dataset.escHandler) {&#xD;&#xA;        document.removeEventListener(&#39;keydown&#39;, img.dataset.escHandler);&#xD;&#xA;      }&#xD;&#xA;      if (img.dataset.clickHandler) {&#xD;&#xA;        document.removeEventListener(&#39;click&#39;, img.dataset.clickHandler);&#xD;&#xA;      }&#xD;&#xA;      &#xD;&#xA;      &#xD;&#xA;      setTimeout(() =&gt; {&#xD;&#xA;        img.style.removeProperty(&#39;--zoom-scale&#39;);&#xD;&#xA;      }, 300);&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  &#xD;&#xA;  document.addEventListener(&#39;touchstart&#39;, function(e) {&#xD;&#xA;    if (e.target.tagName === &#39;IMG&#39; &amp;&amp; e.target.classList.contains(&#39;zoomed&#39;)) {&#xD;&#xA;      e.preventDefault();&#xD;&#xA;      zoomImage(e.target);&#xD;&#xA;    }&#xD;&#xA;  }, { passive: false });&#xD;&#xA;  &lt;/script&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;style&gt;&#xD;&#xA;  img.zoomed {&#xD;&#xA;    position: fixed;&#xD;&#xA;    top: 50%;&#xD;&#xA;    left: 50%;&#xD;&#xA;    transform: translate(-50%, -50%) scale(var(--zoom-scale, 0.95));&#xD;&#xA;    max-width: min(95vw, 1200px);&#xD;&#xA;    max-height: min(95vh, 1200px);&#xD;&#xA;    z-index: 10000;&#xD;&#xA;    cursor: zoom-out;&#xD;&#xA;    background: #fff;&#xD;&#xA;    padding: 1rem;&#xD;&#xA;    border-radius: 12px;&#xD;&#xA;    box-shadow: &#xD;&#xA;      0 0 0 100vmax rgba(0, 0, 0, 0.92),&#xD;&#xA;      0 25px 50px rgba(0, 0, 0, 0.4);&#xD;&#xA;    object-fit: contain;&#xD;&#xA;    transition: all 0.3s cubic-bezier(0.25, 0.46, 0.45, 0.94);&#xD;&#xA;    animation: imageZoomIn 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  @keyframes imageZoomIn {&#xD;&#xA;    from {&#xD;&#xA;      opacity: 0;&#xD;&#xA;      transform: translate(-50%, -50%) scale(0.7);&#xD;&#xA;      filter: blur(10px);&#xD;&#xA;    }&#xD;&#xA;    to {&#xD;&#xA;      opacity: 1;&#xD;&#xA;      transform: translate(-50%, -50%) scale(var(--zoom-scale, 0.95));&#xD;&#xA;      filter: blur(0);&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;   &#xD;&#xA;  @media (prefers-color-scheme: dark) {&#xD;&#xA;    img.zoomed {&#xD;&#xA;      background: #1a1a1a;&#xD;&#xA;      border: 1px solid #333;&#xD;&#xA;      box-shadow: &#xD;&#xA;        0 0 0 100vmax rgba(0, 0, 0, 0.96),&#xD;&#xA;        0 25px 50px rgba(0, 0, 0, 0.6);&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;   &#xD;&#xA;  @media (max-width: 768px) {&#xD;&#xA;    img.zoomed {&#xD;&#xA;      --zoom-scale: 0.85 !important;&#xD;&#xA;      max-width: 98vw;&#xD;&#xA;      max-height: 98vh;&#xD;&#xA;      padding: 0.5rem;&#xD;&#xA;      border-radius: 8px;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;   &#xD;&#xA;  .zoom-active {&#xD;&#xA;    overflow: hidden !important;&#xD;&#xA;    position: fixed;&#xD;&#xA;    width: 100%;&#xD;&#xA;    height: 100%;&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;   &#xD;&#xA;  body {&#xD;&#xA;    transition: overflow 0.3s ease;&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;   &#xD;&#xA;  .img-zoom-close {&#xD;&#xA;    position: fixed;&#xD;&#xA;    top: 20px;&#xD;&#xA;    right: 20px;&#xD;&#xA;    z-index: 10001;&#xD;&#xA;    background: rgba(0, 0, 0, 0.8);&#xD;&#xA;    color: white;&#xD;&#xA;    border: none;&#xD;&#xA;    border-radius: 50%;&#xD;&#xA;    width: 40px;&#xD;&#xA;    height: 40px;&#xD;&#xA;    cursor: pointer;&#xD;&#xA;    font-size: 1.2rem;&#xD;&#xA;    display: flex;&#xD;&#xA;    align-items: center;&#xD;&#xA;    justify-content: center;&#xD;&#xA;    transition: all 0.3s ease;&#xD;&#xA;    opacity: 0;&#xD;&#xA;    animation: fadeIn 0.5s ease 0.2s forwards;&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  .img-zoom-close:hover {&#xD;&#xA;    background: rgba(0, 0, 0, 1);&#xD;&#xA;    transform: scale(1.1);&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  @keyframes fadeIn {&#xD;&#xA;    to { opacity: 1; }&#xD;&#xA;  }&#xD;&#xA;  &lt;/style&gt;&#xA;&lt;h3 id=&#34;flowchart-lr&#34;&gt;flowchart LR&lt;/h3&gt;&#xA;&#xD;&#xA;&#xD;&#xA;&lt;div class=&#34;mermaid&#34; style=&#34;background:transparent&#34;&gt;&#xD;&#xA;  &#xD;&#xA;flowchart LR&#xD;&#xA;    y(&#34;üë´ You&#34;) --&gt; h{&#34;ü§ù Found this helpful?&#34;}&#xD;&#xA;    h --&gt; |Yes| r[/&#34;‚≠ê Check out my featured posts!&#34;/]&#xD;&#xA;    h --&gt; |No| su[/&#34;üìù Suggest changes by clicking near the title&#34;/]&#xD;&#xA;    click r &#34;/categories/featured&#34; _blank&#xD;&#xA;&#xD;&#xA;&lt;/div&gt;&#xA;&lt;h3 id=&#34;gitgraph&#34;&gt;gitGraph&lt;/h3&gt;&#xA;&#xD;&#xA;&#xD;&#xA;&lt;div class=&#34;mermaid&#34; style=&#34;background:transparent&#34;&gt;&#xD;&#xA;  &#xD;&#xA;gitGraph&#xD;&#xA;  commit&#xD;&#xA;  branch feature&#xD;&#xA;  checkout feature&#xD;&#xA;  commit&#xD;&#xA;  checkout main&#xD;&#xA;  merge feature&#xD;&#xA;&#xD;&#xA;&lt;/div&gt;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;  &#xD;&#xA;  &#xD;&#xA;&#xD;&#xA;  &#xD;&#xA;    &#xD;&#xA;    &#xD;&#xA;    &#xD;&#xA;&#xD;&#xA;  &#xD;&#xA;&#xD;&#xA;  &#xD;&#xA;    &lt;div class=&#34;embed-container my-4&#34;&gt;&#xD;&#xA;      &lt;iframe &#xD;&#xA;        src=&#34;https://www.youtube.com/embed/dQw4w9WgXcQ&#34; &#xD;&#xA;        frameborder=&#34;0&#34; &#xD;&#xA;        allowfullscreen &#xD;&#xA;        loading=&#34;lazy&#34;&#xD;&#xA;        allow=&#34;accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture&#34;&#xD;&#xA;      &gt;&lt;/iframe&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;  &#xD;&#xA;&#xA;&lt;video controls width=&#34;100%&#34;  &gt;&#xD;&#xA;  &#xD;&#xA;  &#xD;&#xA;  &lt;source src=&#34;http://localhost:1313/images/content/tutorial/html/image-link.webm&#34; type=&#34;video/mp4&#34;&gt;&#xD;&#xA;  &#xD;&#xA;  &#xD;&#xA;  Browser tidak mendukung video HTML5.&#xD;&#xA;&lt;/video&gt;&#xA;&lt;h3 id=&#34;untuk-progress-tutorial&#34;&gt;Untuk Progress tutorial&lt;/h3&gt;&#xA;&lt;p&gt;kode di bawah ini untuk progress tutorial yang masih belom selesai tambahkan di didalam file layouts:&#xA;layouts/components/daftar-tutorial.html bisa ditambah kedalam file setelah tag &lt;co